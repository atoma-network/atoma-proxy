POSTGRES_DB=atoma_proxy_db
POSTGRES_USER=atoma_proxy_user
POSTGRES_PASSWORD=
POSTGRES_PORT=5432

TRACE_LEVEL=info
ATOMA_PROXY_CONFIG_PATH=./config.toml

ACME_EMAIL=

# Dashboard
DASHBOARD_PORT=3000

# Atoma Proxy
ATOMA_PROXY_SERVICE_PORT=8081

# Atoma API
ATOMA_API_SERVICE_PORT=8080

# Next js credentials api url (the default domain for the Atoma's proxy cloud service api)
NEXT_PUBLIC_PROXY_URL=https://credentials.atomacloud.com 

# Next js credentials api wallet address
NEXT_PUBLIC_PROXY_WALLET=

# Next js credentials api usdc type
NEXT_PUBLIC_USDC_TYPE= # e.g. for testnet '0xa1ec7fc00a6f40db9693ad1415d0c193ad3906494428cf252621037bd7117e29::usdc::USDC'
